<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Panel
		headerText="{i18n>title}"
		class="sapUiResponsiveMargin"
		width="auto" >
		<content>
			<Button
				icon="sap-icon://world"
				text="{i18n>title}"
				press="onOpenDialog"
				class="sapUiSmallMarginEnd" />
		</content>
	</Panel>
</mvc:View>